# Universal ADK Starter Kit Configuration
# Project Configuration
PROJECT_ID={{project_id}}
LOCATION={{location|default:us-central1}}
NAMESPACE={{namespace}}
AGENT_NAME={{agent_name}}

# Model Configuration  
MODEL_NAME={{model_name|default:gemini-2.0-flash}}
GOOGLE_GENAI_USE_VERTEXAI={{use_vertex|default:true}}

# Original configuration from source agent:
# Extracted from google/adk-samples/python/agents/llm-auditor/
GOOGLE_CLOUD_PROJECT={{project_id}}
GOOGLE_CLOUD_LOCATION={{location|default:us-central1}}

# Optional: For local development without Vertex AI
# GOOGLE_GENAI_USE_VERTEXAI=false
# GOOGLE_API_KEY=your-api-key-here

# For deployment to Agent Engine
GOOGLE_CLOUD_STORAGE_BUCKET={{project_id}}-adk-agents
